 #include <iostream>
using namespace std;
int main()
{
  float a,b,c;

  cout<<"Ingrese 3 numeros:\n";cin>>a>>b>>c;
  if(a==b==c){
         cout<<"Todos los numeros son iguales";
         }
         else{
             if(a>b){
             cout<<"el numero mayor es a";
             }
             if(b>c>a){
             cout<<"el numero mayor es b";
             }
             if(c>b>a){
             cout<<"el numero mayor es c";
             }
         }

         return 0;
  }
